@{
    Layout = "Shared/_Layout.cshtml";
}
<h2>@Model["course"].GetName()</h2>
<h3>@Model["course"].GetCourseNumber()</h3>
<h2>Students in this Course:</h2>
<ul>
    @foreach (var student in Model["allStudents"])
    {
        <li><a href="/students/@student.GetId()">@student.GetName()</a></li>
    }
</ul>

<h4><a href="/departments/new">Add a new course</a></h4>
<h4><a href="/">Return home</a></h4>
